public:
    int evenlyDivides(int N){
        //Digit-17
        int temp,digit,count=0;
        temp=N;
        while(temp!=0)
        {
            digit=temp%10;   //7
            temp=temp/10;   //1
            if(digit>0 && N%digit==0)
            {
                count++;
            }
        }
        return count;
    }
};
