class Solution
{
	public: 
	vector<int> find_sum(int n)
		{
		   vector<int>v;
		    int i,sumodd=0,sumeven=0;
		    for(i=1;i<=n;i++)
		    {
		        if(i%2==0)
		        {
		            sumeven=sumeven+i;
		        }
		        else
		        {
		            sumodd=sumodd+i;
		        }
		        
		    }
		    v.push_back(sumodd);
		    v.push_back(sumeven);
		    return v;

		}
};
